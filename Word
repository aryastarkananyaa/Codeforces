#include<bits/stdc++.h>
using namespace std;
int main(){
string s;
char ch;
cin>>s;
int x,i,j=0;  
x=s.length();
int u=0,l=0;
for(i=0;i<x;i++){
if(isupper(s[i]))
u++;
if(islower(s[i])) 
l++;
}
if(u<=l){
while(s[j]){
ch=s[j];
putchar(tolower(ch));
j++;
}
}
else{
while(s[j]){
ch=s[j];
putchar(toupper(ch)); 
j++;
}
}
return 0;
}
